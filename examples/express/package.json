{
  "name": "example-express",
  "version": "1.0.0",
  "description": "Express example for Alpen Dashboard API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "yarn run dev:mjs",
    "build:cjs": "bob-ts -f cjs --paths true",
    "build:esm": "bob-ts -f esm --paths",
    "dev:cjs": "bob-ts-watch -f cjs --paths",
    "dev:esm": "bob-ts-watch ",
    "start": "yarn run compile && node ./dist/index.js",
    "compile": "tsc --build && tsc-alias",
    "start:esm": "bob-ts -i src --paths && node ./dist/index.mjs",
    "typecheck": "tsc --noEmit",
    "types": "tsc --declaration"
  },
  "keywords": [],
  "author": "clayton collie <collie.clayton@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@alpen/demo-base": "link:../base",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.10",
    "bob-ts": "^1.2.2",
    "bob-tsm": "^0.3.5",
    "tsc-alias": "^1.4.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "esbuild": ">=0.14.0"
  }
}
