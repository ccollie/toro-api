{
  "name": "@alpen/demo-cli",
  "version": "1.0.0",
  "description": "Example job producers and consumers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc --build",
    "producer": "node -r ts-node/register src/producer.ts",
    "consumer": "node -r ts-node/register src/consumer.ts"
  },
  "dependencies": {
    "@alpen/core": "^0.0.2",
    "@alpen/shared": "^0.0.2",
    "bullmq": "^1.69.0",
    "lodash-es": "^4.17.21",
    "unirand": "^2.12.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.15",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
