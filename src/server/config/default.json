{
  "percentiles": [25, 50, 75, 90, 95, 99, 99.5],
  "snapshotInterval": 10000,
  "aggregateInterval": 60000,
  "flushInterval": 1000,
  "defaultSlidingWindow": {
    "duration": "2 mins",
    "period": "1 sec"
  },
  "lockTTL": 15000,
  "statsPrefix": "toro",
  "url": "http://localhost:4000",
  "brand": "Toro",
  "title": "Toro Sample Dashboard",
  "server": {
    "host": "http://localhost",
    "port": 4000
  },
  "mail":  {
    "message": {
      "from": "Toro Notifier <my@sender.com>",
      "to": "recipient@example.com",
      "bcc": ["mycc@mail.com"]
    },
    "transport": {
      "jsonTransport": true
    }
  },
  "uriTemplates": {
    "queue": "{{server.host}}:{{server.port}}/queues/{{queue.id}}",
    "host": "{{server.host}}:{{server.port}}/hosts/{{host.id}}"
  },
  "retention": "1 month",
  "hosts": [
    {
      "name": "AWS-Server-2",
      "host": "127.0.0.1",
      "port": 6379,
      "prefix": "bull",
      "queues": [
        {
          "name": "bb_queue",
          "prefix": "bbq",
          "jobTypes": []
        },
        {
          "name": "suzy_queue"
        },
        {
          "name": "widgets"
        },
        {
          "name": "tacos"
        },
        {
          "name": "backup"
        }
      ],
      "channels": [
        {
          "id": "default_hook",
          "type": "webhook",
          "name": "Example Webhook",
          "enabled": false,
          "url": "127.0.0.1:4000/hook?event={{event}}"
        },
        {
          "type": "mail",
          "id": "secondary_mail",
          "name": "Secondary Email",
          "recipients": "recipient@example.com"
        }
      ]
    }
  ]
}
