{
  "description": "Pamplona Sample Dashboard",
  "server": {
    "host": "http://localhost",
    "port": 4000
  },
  "snapshotInterval": 10000,
  "aggregateInterval": 60000,
  "flushInterval": 1000,
  "percentiles": [25, 50, 75, 90, 95, 99, 99.5],
  "hosts": [
    {
      "name": "Localhost",
      "host": "127.0.0.1",
      "port": 6379,
      "prefix": "bull",
      "notifiers": ["console", "mail"],
      "queues": [
        {
          "name": "bb_queue",
          "prefix": "bbq",
          "jobTypes": []
        },
        {
          "name": "tacos"
        },
        {
          "name": "widgets"
        },
        {
          "name": "backup"
        }
      ]
    }
  ],
  "notifiers": [
    {
      "id": "console",
      "type": "console"
    },
    {
      "id": "hook1",
      "type": "webhook",
      "urls": ["{{server.host}}:{{server.port}}/webhooks/notification"],
      "retry": 3,
      "httpSuccessCodes": [200]
    },
    {
      "id": "mail",
      "type": "mail",
      "disable": false,
      "defaults": {
        "to": "flames@example.com",
        "from": "my@sender.com",
        "bcc": ["mycc@mail.com"]
      },
      "transport": {
        "jsonTransport": true
      }
    }
  ]
}
