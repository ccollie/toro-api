{
  "percentiles": [50, 75, 90, 95, 99, 99.5],
  "url": "http://localhost:4000",
  "title": "El-Toro Sample Dashboard",
  "snapshotInterval": 20000,
  "aggregateInterval": 60000,
  "flushInterval": 1000,
  "defaultSlidingWindow": {
    "duration": "2 mins",
    "interval": "1 sec"
  },
  "server": {
    "host": "http://localhost",
    "port": 4000
  },
  "mail":  {
    "message": {
      "from": "Toro Notifier <my@sender.com>",
      "to": "recipient@example.com",
      "bcc": ["mycc@mail.com"]
    },
    "transport": {
      "jsonTransport": true
    }
  },
  "uriTemplates": {
    "queue": "{{server.host}}:{{server.port}}/queues/{{{queue.id}}}",
    "host": "{{server.host}}:{{server.port}}/hosts/{{{host.id}}}"
  },
  "hosts": [
    {
      "name": "develop",
      "connection": {
        "host": "127.0.0.1",
        "port": 6379
      },
      "prefix": "bull",
      "queues": [
        {
          "name": "bb_queue",
          "prefix": "bbq"
        },
        {
          "name": "suzy_queue"
        },
        {
          "name": "widgets"
        },
        {
          "name": "tacos"
        },
        {
          "name": "backup"
        }
      ],
      "channels": [
        {
          "id": "hook1",
          "type": "webhook",
          "name": "Sample webhook",
          "url": "{{server.host}}:{{server.port}}/webhooks/notification",
          "retry": 3
        },
        {
          "id": "mail",
          "type": "mail",
          "name": "Sample mail channel",
          "recipients": "flames@example.com"
        }
      ]
    }
  ]
}
