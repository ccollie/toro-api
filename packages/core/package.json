{
  "name": "@alpen/core",
  "version": "0.0.2",
  "author": "clayton collie <collie.clayton@gmail.com>",
  "license": "MIT",
  "description": "Core implementation for Alpen",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ccollie/alpen.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/ccollie/alpen/issues"
  },
  "homepage": "https://github.com/ccollie/alpen#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "build": "yarn run clean && yarn run compile && yarn run copyAssets",
    "compile": "tsc --build tsconfig.build.json",
    "replace-paths": "tsc-alias -p ./tsconfig.build.json",
    "copy:lua": "copyfiles -f ./src/commands/*.lua ./dist/commands",
    "copy:lua:includes": "copyfiles -f ./src/commands/includes/*.lua ./dist/commands/includes",
    "copy:config": "copyfiles -f ./src/config/*.json ./dist/config",
    "copy:pkg": "copyfiles -f ./package.json ./dist",
    "copy:templates": "copyfiles -f ./src/notifications/email/templates/*.mjml ./dist/notifications/nodemailer/email/templates",
    "copyAssets": "yarn run copy:lua && yarn run copy:lua:includes && yarn run copy:templates && yarn run copy:config",
    "test": "jest",
    "test:coverage": "yarn run clean && jest --coverage",
    "clean": "rm -rf ./dist",
    "typecheck": "tsc --noEmit",
    "types": "tsc --declaration --emitDeclarationOnly",
    "prepack": "yarn run build",
    "prettier": "prettier --config package.json src/**/*.ts",
    "prettier:fix": "prettier --write src/"
  },
  "devDependencies": {
    "@babel/runtime": "^7.16.3",
    "@types/boom": "^7.3.1",
    "@types/glob": "^7.2.0",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/hapi__joi": "^17.1.7",
    "@types/html-to-text": "^8.0.1",
    "@types/ioredis": "^4.28.2",
    "@types/lodash": "^4.14.176",
    "@types/lru-cache": "^5.1.1",
    "@types/mjml": "^4.7.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.11.6",
    "@types/nodemailer": "^6.4.4",
    "copyfiles": "^2.4.1",
    "create-test-server": "^3.0.1",
    "jest": "^27.4.4",
    "jest-date-mock": "^1.0.8",
    "nock": "^13.2.1",
    "tsc-alias": "^1.4.2",
    "unirand": "^2.12.2",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@alpen/shared": "^0.0.2",
    "@datadog/sketches-js": "^1.0.4",
    "@hapi/boom": "^9.1.4",
    "ajv": "^8.8.2",
    "ajv-errors": "^3.0.0",
    "bullmq": "^1.59.1",
    "core-js": "^3.20.0",
    "cronstrue": "^1.122.0",
    "date-fns": "^2.27.0",
    "debug": "^4.3.2",
    "denque": "^2.0.1",
    "dotenv": "^10.0.0",
    "emittery": "^0.10.0",
    "fnv-plus": "^1.3.1",
    "glob": "^7.2.0",
    "got": "^11.8.2",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "handlebars-utils": "^1.0.6",
    "hdr-histogram-js": "^2.0.1",
    "html-to-text": "^8.1.0",
    "ioredis": "^4.28.2",
    "joi": "^17.5.0",
    "lodash-es": "^4.17.21",
    "lru-cache": "^6.0.0",
    "mjml": "4.11.0",
    "ms": "^2.1.3",
    "msgpackr": "^1.4.6",
    "nanoid": "^3.1.30",
    "nodejs-snowflake": "^1.6.2",
    "nodemailer": "^6.7.2",
    "p-all": "^3.0.0",
    "p-map": "4.0.0",
    "p-queue": "^6.6.2",
    "p-settle": "^4.1.1",
    "pino": "^7.5.1",
    "pino-pretty": "^7.3.0",
    "pretty-ms": "^7.0.1",
    "prexit": "^0.0.5",
    "redlock": "^4.2.0",
    "remark": "^13.0.0",
    "remark-html": "^13.0.2",
    "slackify-markdown": "^4.3.0",
    "to-json-schema": "^0.2.5",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "typescript": ">=4.5.4"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80
  }
}
