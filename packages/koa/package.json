{
  "name": "@alpen/koa",
  "version": "1.0.0",
  "description": "Koa adapter for Alpen",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "bob-ts -i src -f interop",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "bullmq",
    "Koa",
    "graphql"
  ],
  "author": "clayton collie",
  "license": "MIT",
  "peerDependencies": {
    "koa": "2.x",
    "koa-router": "7.x || 8.x || 9.x || 10.x"
  },
  "dependencies": {
    "@alpen/core": "^0.0.2",
    "@alpen/api": "^0.0.2",
    "@graphql-ez/koa": "^0.8.7",
    "graphql-ez": "^0.13.1"
  },
  "devDependencies": {
    "bob-ts": "^1.1.2",
    "@types/koa": "^2.13.4",
    "@types/node": "^14.17.17",
    "typescript": "^4.4.3"
  }
}
