{
  "name": "alpen-testing",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "clayton collie <collie.clayton@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bob-esbuild watch",
    "build": "yarn run declarations && bob-ts -i src -f interop",
    "typecheck": "tsc --noEmit",
    "declarations": "tsc --declaration",
    "prepack": "bob-esbuild build"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "typescript": "^4.4.4",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "bob-ts": "^1.2.1",
    "esbuild": "^0.13.12",
    "bob-esbuild-cli": "^3.0.0"
  },
  "dependencies": {
    "fs-extra": "^10.0.0"
  }
}
