{
  "name": "@alpen/express",
  "version": "0.0.2",
  "description": "Express handler for Alpen",
  "keywords": [
    "bullmq",
    "express",
    "graphql"
  ],
  "author": "clayton collie <collie.clayton@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccollie/toro-api.git",
    "directory": "packages/express"
  },
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "bob-ts -i src -f interop"
  },
  "dependencies": {
    "@alpen/core": "^0.0.2",
    "@alpen/api": "^0.0.2",
    "@graphql-ez/express": "^0.9.5",
    "graphql-ez": "^0.13.1"
  },
  "peerDependencies": {
    "express": "4.x",
    "esbuild": ">=0.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "express": "^4.17.1",
    "bob-ts": "^1.1.2",
    "graphql": "15.4.0-experimental-stream-defer.1"
  }
}
