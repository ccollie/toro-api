{
  "name": "@alpen/fastify",
  "version": "0.1.0",
  "description": "Fastify adapter for Alpen",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc --build tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "types": "tsc --declaration --emitDeclarationOnly",
    "test": "jest"
  },
  "keywords": [
    "bullmq",
    "fastify",
    "graphql"
  ],
  "author": "clayton collie <collie.clayton@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@alpen/api": "^0.0.2",
    "@alpen/core": "^0.0.2",
    "@graphql-ez/fastify": "^0.9.4",
    "graphql": "16.1.0",
    "graphql-ez": "^0.13.5"
  },
  "peerDependencies": {
    "fastify": "3.x"
  },
  "devDependencies": {
    "@types/node": "^14.17.5",
    "bob-ts": "^1.2.2",
    "esbuild": "^0.14.0",
    "fastify": "^3.24.0",
    "tsc-alias": "^1.4.1"
  }
}
