{
  "name": "@alpen/shared",
  "version": "0.0.2",
  "private": true,
  "description": "Shared utilities for Alpen",
  "author": "clayton collie <collie.clayton@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "keywords": [
    "bullmq",
    "graphql",
    "library"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "yarn run clean && yarn run types && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "bob-ts -i src -f interop --paths",
    "compile1": "tsc --build && tsc-alias",
    "dev": "bob-ts-watch -i src",
    "types": "tsc --declaration",
    "typecheck": "tsc --noEmit",
    "test": "test",
    "prepack": "bob-esbuild build"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "ajv-errors": "^3.0.0",
    "date-fns": "^2.27.0",
    "jsep": "^1.2.0",
    "lodash-es": "^4.17.21",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.4",
    "bob-ts": "^1.2.2",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "esbuild": ">=0.14.0"
  }
}
